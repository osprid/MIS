<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <system.web>
    <compilation debug="true" targetFramework="4.0">
      <assemblies>
        <add assembly="System.Data.Entity, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      </assemblies>
    </compilation>
  </system.web>
  <connectionStrings>
   <add name="TMSEntities" connectionString="metadata=res://*/TMS.csdl|res://*/TMS.ssdl|res://*/TMS.msl;provider=System.Data.SqlClient;
		provider connection string=&quot;data source=OSPRID-PC\OSPRIDSERVER;
		initial catalog=TMS;integrated security=True;User Id=sa;
		Password=pass123;multipleactiveresultsets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
	  <!--<add name="TMSEntities" connectionString="metadata=res://*/TMS.csdl|res://*/TMS.ssdl|res://*/TMS.msl;provider=System.Data.SqlClient;provider connection string=&quot;
	  data source=.\sqlserver;initial catalog=TMS;integrated security=True;multipleactiveresultsets=True;App=EntityFramework&quot;"
	  providerName="System.Data.EntityClient" />-->
  </connectionStrings>
  <log4net debug="true">
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="ErrorLog\\TestLog.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="10MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5p %d %5rms %-22.22c{1} %-18.18M - %m%n" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingLogFileAppender" />
    </root>
  </log4net>
</configuration>